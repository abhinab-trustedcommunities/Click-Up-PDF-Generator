        color: #b5b5b5;
        text-align: center;
        width: 16px;
      }
      th .mname {
        color: #8d96a3;
      }

      .svgname h2 {
        font-size: 16px;
        color: black;
        text-align: left;
        padding-left: 25px;
      }

      button {
        background: #11101d;
        color: white;
        margin: 3% 50%;
        padding: 10px 10px 10px 10px;
        border-radius: 15px;
        display: flex;
        border: solid 1px #11101d;
        font-size: 90%;
        flex-wrap: wrap;
        justify-content: center;
        align-items: center;
      }

      p.path {
        color: #a7a6a6;
        display: block;
        text-align: left;
        margin-bottom: 10px;
      }

      path#red {
        fill: red;
      }

